{
  "extends": "next/core-web-vitals",
  "rules": {
    // "@typescript-eslint/no-explicit-any": "warn",
    "jsx-a11y/anchor-is-valid": "off"
  },
  "settings": {
    "import/resolver": {
      "typescript": {
        "project": "./tsconfig.json"
      }
    }
  },
  "overrides": [
    {
      "files": ["**/*.(d.)?ts(x)?"],
      "parser": "@typescript-eslint/parser",
      "parserOptions": {
        "project": "./tsconfig.json"
      },
      "rules": {}
    }
  ]
}
